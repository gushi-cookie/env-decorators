{
  "name": "env-decorators",
  "version": "0.0.1",
  "description": "This packages allows you to associate class properties with environment variables, via TypeScript decorators.",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
    "./decorators": {
      "types": "./build/decorators/index.d.ts",
      "default": "./build/decorators/index.js"
    },
    "./errors": {
      "types": "./build/errors/index.d.ts",
      "default": "./build/errors/index.js"
    },
    "./metadata": {
      "types": "./build/metadata/index.d.ts",
      "default": "./build/metadata/index.js"
    },
    "./resolver": {
      "types": "./build/resolver/index.d.ts",
      "default": "./build/resolver/index.js"
    },
    "./utils": {
      "types": "./build/utils/index.d.ts",
      "default": "./build/utils/index.js"
    }
  },
  "scripts": {
    "prettify": "prettier --write .",
    "lint": "eslint && npm run check-spell",
    "test": "jest",
    "build": "tsc",
    "check-spell": "cspell .",
    "prepare": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gushi-cookie/env-decorators.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "cspell": "^8.17.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  }
}
